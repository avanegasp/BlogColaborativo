<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <%@post.each do |post|%>
      <div class="container-post">
        <h2> <%= post.title %></h2>
        <p> <%= post.created_at %></p>
        <p> <%= post.user.email%></p>
        <p> <%=post.body %></p>
        <%= link_to "ir al show", post_path(post.id) %>
      </div>
      <%end%>
    </div>
  </div>
</div>
